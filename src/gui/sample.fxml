<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane minHeight="660.0" minWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Controller">
    <center>
        <TextArea promptText="logi symulacji" BorderPane.alignment="CENTER">
            <font>
                <Font size="36.0" />
            </font></TextArea>
    </center>
    <left>
    </left>
    <top>
    </top>
    <right>
        <VBox prefHeight="589.0" prefWidth="411.0" BorderPane.alignment="CENTER">
            <children>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label text="simulation parameters">
                            <font>
                                <Font size="23.0" />
                            </font>
                        </Label>
                    </children>
                </HBox>
                <GridPane fx:id="gridPane">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="270.0" minWidth="10.0" prefWidth="257.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="154.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label text="maxQueueSize" />
                        <Label text="percentageOfCustomersDoingSmallShopping" GridPane.rowIndex="1" />
                        <Label text="initialNumberOfCheckouts" GridPane.rowIndex="2" />
                    </children>
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" />
                    </VBox.margin>
                </GridPane>
                <HBox alignment="CENTER" prefHeight="64.0" prefWidth="411.0">
                    <children>
                        <Button fx:id= "startSimulationBtn" mnemonicParsing="false" onAction="#startSimulation" text="start simulation">
                            <opaqueInsets>
                                <Insets />
                            </opaqueInsets>
                            <HBox.margin>
                                <Insets right="5.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id= "stopSimulationBtn" mnemonicParsing="false" onAction="#stopSimulation" text="stop simulation">
                            <HBox.margin>
                                <Insets left="5.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </right>
</BorderPane>
